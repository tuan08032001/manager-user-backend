{
  "name": "os.api.document",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "build": "npm run build-ts && npm run lint",
    "build-ts": "tsc",
    "debug": "npm run build && npm run watch-debug",
    "lint": "tsc --noEmit && eslint \"**/*.{js,ts}\" --quiet --fix",
    "serve": "node dist/server.js",
    "serve-debug": "nodemon --inspect dist/server.js",
    "start": "npm run serve",
    "test": "jest --forceExit --coverage --verbose",
    "watch": "concurrently -k -p \"[{name}]\" -n \"TypeScript,Node\" -c \"yellow.bold,cyan.bold,green.bold\" \"npm run watch-ts\" \"npm run watch-node\"",
    "watch-debug": "concurrently -k -p \"[{name}]\" -n \"TypeScript,Node\" -c \"yellow.bold,cyan.bold,green.bold\" \"npm run watch-ts\" \"npm run serve-debug\"",
    "watch-node": "nodemon dist/server.js",
    "watch-test": "npm run test -- --watchAll",
    "watch-ts": "tsc -w",
    "proto-gen": "yarn proto-loader-gen-types --grpcLib=@grpc/grpc-js --keepCase --outDir=src/configs/rpc/protos/ src/configs/rpc/protos/*.proto"
  },
  "dependencies": {
    "@types/crypto-js": "^4.1.1",
    "@types/node": "^20.10.4",
    "bcryptjs": "^2.4.3",
    "bluebird": "^3.7.2",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dayjs": "^1.11.10",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "express-session": "^1.17.2",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.21",
    "log4js": "^6.3.0",
    "module-alias": "^2.2.2",
    "morgan": "^1.10.0",
    "mysql2": "^2.2.5",
    "node-cron": "^3.0.0",
    "node-mailjet": "^6.0.5",
    "nodemailer": "^6.6.5",
    "passport": "^0.7.0",
    "passport-google-oauth2": "^0.2.0",
    "passport-jwt": "^4.0.1",
    "sequelize": "6.6.5",
    "sequelize-cli": "6.2.0",
    "swagger-jsdoc": "^6.1.0",
    "swagger-ui-express": "^4.1.6",
    "ts-node": "^10.2.1",
    "tsconfig-paths": "^3.10.1",
    "typescript": "^4.3.5",
    "yargs": "^17.0.1"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/bluebird": "^3.5.36",
    "@types/compression": "^1.7.5",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/express-session": "^1.17.4",
    "@types/jsonwebtoken": "^8.5.5",
    "@types/lodash": "^4.14.171",
    "@types/morgan": "^1.9.3",
    "@types/passport": "^1.0.16",
    "@types/passport-google-oauth2": "^0.1.8",
    "@types/passport-jwt": "^3.0.13",
    "@types/swagger-jsdoc": "^6.0.1",
    "@types/swagger-ui-express": "^4.1.3",
    "@types/validator": "^13.6.3",
    "@typescript-eslint/eslint-plugin": "7",
    "@typescript-eslint/parser": "7",
    "concurrently": "^6.2.1",
    "eslint": "^8",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^18.0.0",
    "eslint-config-standard": "17",
    "eslint-plugin-import": "^2.24.2",
    "eslint-plugin-n": "^16.6.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "6",
    "nodemon": "^2.0.9"
  },
  "_moduleAliases": {
    "@configs": "dist/configs",
    "@database": "dist/database",
    "@entities": "dist/entities",
    "@initializers": "dist/initializers",
    "@interfaces": "dist/interfaces",
    "@models": "dist/models",
    "@services": "dist/services",
    "@controllers": "dist/controllers",
    "@middlewares": "dist/middlewares",
    "@workers": "dist/workers",
    "~": "dist",
    "@libs": "dist/libs",
    "@decorators": "dist/decorators",
    "@jobs": "dist/jobs"
  },
  "xo": {
    "extends": ".eslintrc.json"
  }
}
